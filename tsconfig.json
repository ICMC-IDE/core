{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ES2022",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "noEmit": true,
    "declaration": true,
    "outDir": "dist",
    "lib": ["DOM", "DOM.Iterable", "DOM.AsyncIterable", "ES2022"]
  }
}
